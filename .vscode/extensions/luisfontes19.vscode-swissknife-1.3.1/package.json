{
	"name": "vscode-swissknife",
	"displayName": "Swissknife",
	"description": "The developers swissknife. Do convertions and generations right out of vs code. Extendable with user scripts",
	"version": "1.3.1",
	"author": "luisfontes19",
	"publisher": "luisfontes19",
	"license": "MIT",
	"engines": {
		"vscode": "^1.50.0"
	},
	"repository": {
		"url": "https://github.com/luisfontes19/vscode-swissknife"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:swissknife.show",
		"onCommand:swissknife.reload",
		"onCommand:swissknife.openScripts"
	],
	"main": "./out/extension.js",
	"contributes": {
		"menus": {
			"touchBar": [
				{
					"command": "swissknife.show",
					"group": "editing"
				}
			]
		},
		"commands": [
			{
				"command": "swissknife.show",
				"title": "Open Swissknife command pallete",
				"icon": "data/icon.png"
			},
			{
				"command": "swissknife.reload",
				"title": "Reload Swissknife scripts"
			},
			{
				"command": "swissknife.openScripts",
				"title": "Open Swissknife user scripts folder"
			}
		],
		"keybindings": [
			{
				"command": "swissknife.show",
				"key": "Ctrl+Shift+9",
				"mac": "Cmd+Shift+9",
				"when": "editorFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run bundle",
		"vscode:package": "vsce package",
		"bundle": "ncc build src/extension.ts -o out/extension.js",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/bcrypt": "^3.0.0",
		"@types/elliptic": "^6.4.12",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/request": "^2.48.5",
		"@types/uuid": "^8.3.0",
		"@types/vscode": "^1.50.0",
		"@types/zxcvbn": "^4.4.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"@types/bip39": "^3.0.0",
		"@types/express": "^4.17.11",
		"eslint": "^7.9.0",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"bcrypt": "^5.0.1",
		"elliptic": "^6.5.4",
		"express": "^4.17.1",
		"glob": "^7.1.6",
		"hash-identifier": "^1.0.1",
		"request": "^2.88.2",
		"selfsigned": "^1.10.8",
		"showdown": "^1.9.1",
		"uuid": "^8.3.1",
		"vscode-swissknife": "^0.0.2",
		"zxcvbn": "^4.4.2"
	},
	"keywords": [
		"server",
		"serve",
		"sort",
		"swissknife",
		"scriptable",
		"encode",
		"decode",
		"convertion",
